$('#reviews').html('<%= escape_javascript render @reviews %>');
$('.paginate').html('<%= escape_javascript(paginate(@reviews, remote: true).to_s) %>');
$('.num_entries').html('<%= escape_javascript(page_entries_info(@reviews, remote: true).to_s) %>');

$('.star-rating').raty({
  path: '',
  readOnly: true,
  score: function() { return $(this).attr('data-score') }
});